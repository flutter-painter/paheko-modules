{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"properties": {
		"invoice_text": {
			"type": ["null", "string"]
		},
		"quote_text": {
			"type": ["null", "string"]
		},
		"users_address_fields": {
			"description": "Fields used for the address of the user",
			"type": ["null", "array"],
			"items": {
				"type": "string"
			}
		},
		"first_invoice_number": {
			"type": "integer"
		},
		"first_quote_number": {
			"type": "integer"
		}
	},
	"required": ["invoice_text", "quote_text", "users_address_fields"]
}

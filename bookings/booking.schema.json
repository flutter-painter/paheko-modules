{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"properties": {
		"slot": {
			"type": "string"
		},
		"event": {
			"type": "string"
		},
		"date": {
			"type": "string",
			"format": "datetime"
		},
		"name": {
			"type": "string"
		},
		"email": {
			"type": ["null", "string"]
		},
		"fields": {
			"type": ["null", "array"],
			"items": {
				"type": "object",
				"properties": {
					"label": {
						"type": "string"
					},
					"value": {
						"type": "string"
					}
				}
			}
		}
	},
	"required": ["date", "slot", "event", "name", "email", "fields"]
}